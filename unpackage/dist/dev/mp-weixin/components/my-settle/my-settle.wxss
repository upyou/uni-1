/* 水平间距 */
.my-settle {
  display: flex;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 50px;
  justify-content: space-between;
  align-items: center;
  padding: 0 5px;
}
.my-settle .btn-settle {
  width: 100px;
  text-align: center;
  line-height: 50px;
  background-color: #C00000;
  color: white;
}